rror,\n    message,\n    inputValid\n  }\n}\n\n/**\n * @param {*} initialValue\n * @param {*} validations\n*/\n\nconst useInputValidation = (initialValue, validations, nameInput) => {\n  const [value, setValue] = React.useState(initialValue)\n  const [isDirty, setDirty] = React.useState(false)\n  const valid = useValidation(value, validations, nameInput)\n  const onChange = (e) => {\n    setValue(e.target.value)\n  }\n  const onBlur = (e) => {\n    setDirty(true)\n  }\n  return {\n    value,\n    onChange,\n    onBlur,\n    isDirty,\n    ...valid\n  }\n}\n\nexport default useInputValidation","/home/hp/my_test_task/src/app/components/ErrorMessage.js",[],"/home/hp/my_test_task/src/app/screens/Users/FormNewUser.js",[],"/home/hp/my_test_task/src/app/store/types.js",[],"/home/hp/my_test_task/src/app/components/TokenMessage.js",[],{"ruleId":"98","severity":1,"message":"99","line":5,"column":8,"nodeType":"100","messageId":"101","endLine":5,"endColumn":13},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"98","severity":1,"message":"106","line":1,"column":10,"nodeType":"100","messageId":"101","endLine":1,"endColumn":34},{"ruleId":"102","replacedBy":"107"},{"ruleId":"104","replacedBy":"108"},{"ruleId":"102","replacedBy":"109"},{"ruleId":"104","replacedBy":"110"},{"ruleId":"102","replacedBy":"111"},{"ruleId":"104","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":33,"column":25,"nodeType":"115","messageId":"116","endLine":33,"endColumn":27},{"ruleId":"117","severity":1,"message":"118","line":21,"column":46,"nodeType":"119","messageId":"120","endLine":21,"endColumn":48},{"ruleId":"98","severity":1,"message":"121","line":28,"column":9,"nodeType":"100","messageId":"101","endLine":28,"endColumn":20},{"ruleId":"98","severity":1,"message":"122","line":11,"column":18,"nodeType":"100","messageId":"101","endLine":11,"endColumn":27},{"ruleId":"98","severity":1,"message":"123","line":11,"column":10,"nodeType":"100","messageId":"101"