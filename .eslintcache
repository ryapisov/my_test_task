> firstName.onChange(e)}\n        placeholder=\"firstname\"\n        name=\"firstname\"\n        disabled={!isEdit}\n      />\n      <Input\n        value={lastName.value} \n        onBlur={(e)=> lastName.onBlur(e)}\n        onChange={(e)=> lastName.onChange(e)}\n        placeholder=\"lastName\"\n        name=\"lastName\"\n        disabled={!isEdit}\n      />\n    {\n    !isEdit ?\n      <Button\n        title=\"Edit\"\n        onClick={()=>setIsEdit(!isEdit)} \n      ><img src={iconEditSvg} />\n      </Button>\n    :\n      <>\n        <Button\n          type=\"submit\"\n          title=\"Save\"\n          disabled={\n            !userName.inputValid || \n            !lastName.inputValid || \n            !firstName.inputValid\n          }\n        ><img src={iconSaveSvg} alt=''/>\n        </Button>\n        <Button \n          type=\"button\"\n          title=\"Delete\"\n          onClick={()=>deleteUserHandler(props.id)}\n        ><img src={iconDeleteSvg} alt=''/>\n        </Button>\n        <Button\n          type=\"button\"\n          title=\"Cancel\" \n          onClick={()=>setIsEdit(!isEdit)} \n        ><img src={iconCloseSvg} alt=''/>\n        </Button>\n      </>\n   }\n  </Form>\n  )\n}\n\nfunction UserList(){\n  const dispatch = useDispatch()\n  const users = useSelector((state)=> state.users.users )\n\n  useEffect(()=>{\n    dispatch(getUsersAPI())\n  }, [dispatch])\n\n  return (\n    <>\n      <h3>Список пользователей</h3>\n      {users.map((user, i)=> <UserItem key={i} {...user} />)}\n    </>\n  )\n}\n\nexport default UserList\n","/home/hp/my_test_task/src/app/containers/NewUser.js",["110","111","112","113"],"/home/hp/my_test_task/src/app/containers/Search.js",[],"/home/hp/my_test_task/src/app/components/index.js",[],"/home/hp/my_test_task/src/app/components/Notice.js",[],"/home/hp/my_test_task/src/app/components/Input.js",[],"/home/hp/my_test_task/src/app/components/Button.js",[],"/home/hp/my_test_task/src/app/components/Form.js",[],{"ruleId":"114","r